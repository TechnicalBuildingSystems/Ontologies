<https://w3id.org/ibp/BasicDatatypeOntology> a owl:Ontology ;
    rdfs:label "Basic Datatype Ontology"^^xsd:string ;
    dcterms:contributor "Ana Roxin"^^xsd:string ;
    dcterms:creator <https://orcid.org/0000-0002-2033-859X>,
        <https://www.researchgate.net/profile/Georg_Schneider3>,
        "Georg Ferdinand Schneider"^^xsd:string ;
    dcterms:description """This ontology aims at modelling the semantics of the basic data types 
    provided by the XSD standard for OWL ontologies. In particular, it provides the possibility 
    to describe basic data types of resources rather then Literals in RDF.

    
    This problem appears to be existent also in other domains such as e-commerce, e.g.
    https://www.w3.org/2011/webschema/track/issues/14
    """^^xsd:string ;
    dcterms:identifier "https://w3id.org/ibp/BasicDatatypeOntology#"^^xsd:string ;
    dcterms:issued "2018-06-20"^^xsd:date ;
    dcterms:license <http://creativecommons.org/licenses/by-sa/4.0> ;
    dcterms:modified "2018-06-20"^^xsd:date ;
    dcterms:rights "Copyright (c) 2017/ 2018, Fraunhofer Institute for Building Physics, Nuremberg, Germany. This ontology is distributed under a Creative Commons Attribution Share Alike License (CC BY-SA 4.0) http://creativecommons.org/licenses/by-sa/4.0"^^xsd:string ;
    dcterms:title "The Basic Data Types ontology."^^xsd:string ;
    vann:preferredNamespacePrefix "BDO"^^xsd:string ;
    rdfs:seeAlso xsd:,
        <https://www.github.com/TechnicalBuildingSystems/Ontologies> ;
    BDO: "BDO"^^xsd:string .

