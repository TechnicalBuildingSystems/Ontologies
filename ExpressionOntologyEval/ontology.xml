    <!-- https://w3id.org/ibp/CTRLont#ApplicationLogic -->


    <owl:Class rdf:about="https://w3id.org/ibp/CTRLont#ApplicationLogic">
        <rdfs:comment xml:lang="en">Concept to specify the application logic of a control actor. Seperate ontologies may be used to further specify this concept.</rdfs:comment>
        <rdfs:label xml:lang="en">Application Logic</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/ibp/CTRLont#ControlActor -->


    <owl:Class rdf:about="https://w3id.org/ibp/CTRLont#ControlActor">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/ibp/CTRLont#hasApplicationLogic"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/ibp/CTRLont#ApplicationLogic"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/ibp/CTRLont#hasOutput"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/ibp/CTRLont#Output"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/ibp/CTRLont#hasApplicationLogic"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/ibp/CTRLont#ApplicationLogic"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/ibp/CTRLont#hasInput"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/ibp/CTRLont#Input"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/ibp/CTRLont#hasOutput"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/ibp/CTRLont#Output"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/ibp/CTRLont#hasParameter"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/ibp/CTRLont#Parameter"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Concept to describe the in general accepted Sense-Process-Actuate
pattern in automation systems, e.g. function blocks from IEC 61499.</rdfs:comment>
        <rdfs:label xml:lang="en">Control Actor</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/ibp/CTRLont#DataType -->


    <owl:Class rdf:about="https://w3id.org/ibp/CTRLont#DataType">
        <rdfs:comment xml:lang="en">Placeholder class for the specification of basic data types such as, e.g. integer, boolean or double.</rdfs:comment>
        <rdfs:label xml:lang="en">Data type</rdfs:label>
        <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://www.w3.org/TR/xmlschema-2/#built-in-datatypes</rdfs:seeAlso>
    </owl:Class>
    


    <!-- https://w3id.org/ibp/CTRLont#Input -->


    <owl:Class rdf:about="https://w3id.org/ibp/CTRLont#Input">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ibp/CTRLont#AnnotatedElement"/>
        <owl:disjointWith rdf:resource="https://w3id.org/ibp/CTRLont#Parameter"/>
        <rdfs:comment xml:lang="en">An input allow flows of information to enter a control actor. Inputs are processed by the respective application logic of the control actor to compute the outputs.</rdfs:comment>
        <rdfs:label xml:lang="en">Input</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/ibp/CTRLont#Medium -->


    <owl:Class rdf:about="https://w3id.org/ibp/CTRLont#Medium">
        <rdfs:comment xml:lang="en">Placeholder concept to stipulate reuse of existing ontologies such as dbpedia or  	http://www.productontology.org/id/</rdfs:comment>
        <rdfs:label xml:lang="en">Medium</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://wiki.dbpedia.org/"/>
        <rdfs:seeAlso rdf:resource="http://www.productontology.org/id/"/>
    </owl:Class>
    


    <!-- https://w3id.org/ibp/CTRLont#Output -->


    <owl:Class rdf:about="https://w3id.org/ibp/CTRLont#Output">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ibp/CTRLont#AnnotatedElement"/>
        <owl:disjointWith rdf:resource="https://w3id.org/ibp/CTRLont#Parameter"/>
        <rdfs:comment xml:lang="en">An output allows compute results by the application logic of an control actor to be fowarded to outside entities such as actuators or other control actors.</rdfs:comment>
        <rdfs:label xml:lang="en">Output</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/ibp/CTRLont#Parameter -->


    <owl:Class rdf:about="https://w3id.org/ibp/CTRLont#Parameter">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ibp/CTRLont#AnnotatedElement"/>
        <rdfs:comment xml:lang="en">Parameter describe time-invariant values and settings of the respective control actor. Thus thay can be added dependent on the respective application logic.</rdfs:comment>
        <rdfs:label xml:lang="en">Parameter</rdfs:label>
    </owl:Class>
    


