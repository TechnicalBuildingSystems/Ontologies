@prefix : <https://w3id.org/ibp/hbc/hex> .
@prefix bot: <https://w3id.org/bot#> .
@prefix exp: <http://linkedearth.org/experience/ns#> .
@prefix hex: <https://w3id.org/ibp/hbc/hex#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <https://w3id.org/def/saref4bldg#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix saref: <https://w3id.org/saref#> .
@prefix bim4hex: <https://w3id.org/ibp/hbc/bim4hex#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix time4hex: <https://w3id.org/ibp/hbc/time4hex#> .
@prefix saref4bldg: <https://w3id.org/def/saref4bldg#> .
@base <https://w3id.org/ibp/hbc/hex> .

<https://w3id.org/ibp/hbc/hex> rdf:type owl:Ontology ;
                                owl:versionInfo "1.0"@en ;
                                <http://purl.org/dc/elements/1.1/license> "<http://creativecommons.org/licenses/by-sa/4.0>" ;
                                rdfs:comment "The ontology formally represents human experiencse about IEQ factors, such as thermal comfort, visual comfort , acoustic comfort and IAQ (Indoor Air Quality). It can intregate with time ontology and bim4hex."@en ;
                                <http://purl.org/dc/elements/1.1/rights> "\"\"Copyright (c) 2017, Fraunhofer Institute for Building Physics,Nuremberg, Germany. This ontology is distributed under a Creative Commons Attribution Share Alike License (CC BY-SA 4.0) http://creativecommons.org/licenses/by-sa/4.0\"\""@en ;
                                <http://purl.org/dc/elements/1.1/description> "OWL ontology for human experience which can be integerated with time ontology and building related ontologies."@en ;
                                <http://purl.org/dc/elements/1.1/language> "en"@en ;
                                <http://purl.org/dc/elements/1.1/date> "30.11.2017"@en ;
                                <http://purl.org/dc/elements/1.1/title> "Hex Ontology - An ontology for descripting Human Experience of Indoor Environment"@en ;
                                <http://purl.org/dc/elements/1.1/creator> "Haonan Qiu"@en ;
                                <http://purl.org/dc/elements/1.1/contributor> "Georg Schneider (georg.schneider@ibp.fraunhofer.de)"@en .

#################################################################
