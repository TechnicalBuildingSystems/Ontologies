#################################################################
#    Classes
#################################################################

###  http://www.w3.org/2006/time#DateTimeDescription
time:DateTimeDescription rdf:type owl:Class .


###  http://www.w3.org/2006/time#Instant
time:Instant rdf:type owl:Class .


###  https://w3id.org/ibp/hbc/time#Afternoon
:Afternoon rdf:type owl:Class ;
           owl:equivalentClass [ owl:intersectionOf ( time:DateTimeDescription
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty time:hour ;
                                                        owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                             owl:intersectionOf ( [ rdf:type rdfs:Datatype ;
                                                                                                    owl:onDatatype xsd:nonNegativeInteger ;
                                                                                                    owl:withRestrictions ( [ xsd:minInclusive "12"^^xsd:nonNegativeInteger
                                                                                                                           ]
                                                                                                                         )
                                                                                                  ]
                                                                                                  [ rdf:type rdfs:Datatype ;
                                                                                                    owl:onDatatype xsd:nonNegativeInteger ;
                                                                                                    owl:withRestrictions ( [ xsd:maxExclusive "19"^^xsd:nonNegativeInteger
                                                                                                                           ]
                                                                                                                         )
